#include <stdio.h>
#include <unistd.h>
#include <sys/types.h>
#include <sys/wait.h>

/**
 * main - super simple shell
 *
 * Return: 0 upon success
 */
int main(void)
{
	int status;
	char *buf;
	char **argv;
	pid_t child_pid;
	size_t buf_size = 0;

	while (1)
	{
		printf("#cisfun$ ");
		/* buf_size will be updated to reflect necessary size */
		getline(&buf, &buf_size, stdin);

		
	}


}
